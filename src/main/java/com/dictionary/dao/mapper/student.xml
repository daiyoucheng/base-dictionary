<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dictionary.dao.studentDao">
    <select id="search" parameterType="com.dictionary.bean.student" resultType="com.dictionary.bean.student">
		SELECT
                *
            FROM
                stuinfo
            WHERE
                id = #{id}
    </select>
</mapper>